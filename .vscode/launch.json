{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Flask",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/server/app.py",
        "env": {
          "FLASK_APP": "${workspaceFolder}/server/app.py",
          "FLASK_ENV": "development"
        },
        "args": ["run"],
        "jinja": true
      },
      {
        "name": "Flutter",
        "request": "launch",
        "type": "dart",
        "program": "${workspaceFolder}/client/lib/main.dart"
      }
    ]
  }
  